{
  "name": "with-redux-saga",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "env-cmd -f .env.development next",
    "dev-sbx": "env-cmd -f .env.sbx next",
    "build": "next build",
    "start": "next start",
    "build-sbx": "env-cmd -f .env.sbx npm run build-css && npm run build",
    "build-prod": "env-cmd -f .env.prod npm run build-css && npm run build",
    "build-css": "node-sass-chokidar ./public ./node_modules -o ./public",
    "watch-css": "npm run build-css && node-sass-chokidar ./public ./node_modules -o ./public --watch --recursive"
  },
  "dependencies": {
    "@svgr/webpack": "^5.5.0",
    "axios": "^0.21.1",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "bootstrap": "^4.6.0",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "fingerprintjs2": "^2.1.4",
    "js-sha256": "^0.9.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "next": "latest",
    "next-redux-wrapper": "^6.0.2",
    "next-translate": "^1.0.6",
    "node-rsa": "^1.1.1",
    "node-sass-chokidar": "^1.5.0",
    "numeral": "^2.0.6",
    "query-string": "^7.0.0",
    "react": "16.13.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "16.13.1",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^7.5.1",
    "react-number-format": "^4.5.5",
    "react-otp-input": "^2.3.0",
    "react-redux": "7.2.0",
    "react-select": "^4.3.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "sass": "^1.32.10",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "redux-devtools-extension": "^2.13.8"
  }
}
